<% include include/head.ejs %>
<% include include/top.ejs %>
    <div class="container">
      <div class="page-header">
        <h1>Guardian</h1>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-md-6 scrollable">
          <h1>Active Sessions</h1>
          <div class="list-group" role="tabpanel">
            <% Object.keys(sessions).forEach(function(id) { -%>
            <a href="#<%= id %>" class="list-group-item" data-toggle="tab"><%= id %></a>
            <% }) -%>
          </div>
        </div>
        <div class="col-md-6">
          <div class="tab-content">
            <% Object.keys(sessions).forEach(function(id) { -%>
            <div class="panel panel-default tab-pane" id="<%= id %>">
              <div class="panel-heading">
                <h3 class="panel-title"><%= id %></h3>
              </div>
              <div class="panel-body">
                <pre><%= JSON.stringify(sessions[id], null, 2) %></pre>
              </div>
            </div>
            <% }) -%>
          </div>
        </div>
      </div>
    </div>
<% include include/bottom.ejs %>
